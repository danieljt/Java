public class Isbod
{
  private int antall;
  private String[] ansatte; 

  public Isbod()
  {
    antall = 0;
    ansatte = new String[antall];
  }

  public void ansett(String navn)
  {
    if(antall + 1 > ansatte.length)
      {
	System.println("Du kan ikke ansette flere")
      }
    else
      {
	ansatte[antall] = navn;
	antall = antall + 1;
      }
  }
  public void giSistemannSparken()
  {
    
  }
}
