import java.util.Arrays;
import java.util.Scanner;

class MinOppgave3
{
  public static void main(String[] args)
  {
    Scanner reader = new Scanner(System.in);

    // Read values for approximation
    System.out.print("Gi x-koordinaten: ");
    double x = reader.nextInt();
    System.out.print("Gi antall summeringer: ");
    int N = reader.nextInt();

    // Run loop
    double[] parts = sin_taylor(x, N);
    System.out.println(Arrays.toString(parts));
  }

  static double[] sin_taylor(double x, int N)
  {
    // Define vector
    double[] parts = new double[N];
    
    // Initial condition
    parts[0] = x;
    int fac = 1;
    
    int i = 1;
    while (i < N)
      { 
	parts[i] += (-1)^i*x^(2*i + 1)/fac;
	i += 1;
	fac = fac*(2*j + 1);
      }
    return parts;
  }
}
